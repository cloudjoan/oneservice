							<label class="col-lg-2 col-form-label">服務團隊</label>
							<div class="col-lg-4">
								@Html.DropDownList("ddl_cTeamID", (IEnumerable<SelectListItem>)ViewBag.ddl_cTeamID, new { @class = "form-control chosen-select", @multiple = "true", @data_placeholder = "請選擇服務團隊" })
							</div>
						</div>						
						<div class="form-group row">							
							<label class="col-lg-2 col-form-label">報修產品序號</label>
